    .btnn i {
        line-height: 60px;
        font-size: 26px;
        transition: 0.2s linear;
    }

    .btnn:hover i {
        transform: scale(1.3);
        color: #f1f1f1;
    }

    .btnn::before {
        content: "";
        position: absolute;
        width: 120%;
        height: 120%;
        background: #007bff;
        transform: rotate(45deg);
        left: -110%;
        top: 90%;
    }

    .btnn:hover::before {
        animation: aaa 0.7s 1;
        top: -10%;
        left: -10%;
    }

    @keyframes aaa {
        0% {
            left: -110%;
            top: 90%;
        }

        50% {
            left: 10%;
            top: -30%;
        }

        100% {
            top: -10%;
            left: -10%;
        }
    }
